#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-function@0.3.4/node_modules/inspect-function/bin/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-function@0.3.4/node_modules/inspect-function/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-function@0.3.4/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-function@0.3.4/node_modules/inspect-function/bin/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-function@0.3.4/node_modules/inspect-function/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-function@0.3.4/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/magicli.js" "$@"
else
  exec node  "$basedir/../../bin/magicli.js" "$@"
fi
