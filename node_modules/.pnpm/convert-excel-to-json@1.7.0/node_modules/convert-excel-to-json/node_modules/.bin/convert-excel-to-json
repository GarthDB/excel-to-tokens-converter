#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/convert-excel-to-json@1.7.0/node_modules/convert-excel-to-json/bin/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/convert-excel-to-json@1.7.0/node_modules/convert-excel-to-json/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/convert-excel-to-json@1.7.0/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/convert-excel-to-json@1.7.0/node_modules/convert-excel-to-json/bin/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/convert-excel-to-json@1.7.0/node_modules/convert-excel-to-json/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/convert-excel-to-json@1.7.0/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
