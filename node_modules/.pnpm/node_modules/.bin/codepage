#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/codepage@1.13.1/node_modules/codepage/bin/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/codepage@1.13.1/node_modules/codepage/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/codepage@1.13.1/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/codepage@1.13.1/node_modules/codepage/bin/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/codepage@1.13.1/node_modules/codepage/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/codepage@1.13.1/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../codepage/bin/codepage.njs" "$@"
else
  exec node  "$basedir/../codepage/bin/codepage.njs" "$@"
fi
