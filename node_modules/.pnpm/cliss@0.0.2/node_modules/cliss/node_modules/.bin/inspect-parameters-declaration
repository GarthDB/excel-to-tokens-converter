#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-parameters-declaration@0.0.9/node_modules/inspect-parameters-declaration/bin/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-parameters-declaration@0.0.9/node_modules/inspect-parameters-declaration/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-parameters-declaration@0.0.9/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-parameters-declaration@0.0.9/node_modules/inspect-parameters-declaration/bin/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-parameters-declaration@0.0.9/node_modules/inspect-parameters-declaration/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/inspect-parameters-declaration@0.0.9/node_modules:/Users/garthdb/Spectrum/excel-convert-tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../inspect-parameters-declaration@0.0.9/node_modules/inspect-parameters-declaration/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../inspect-parameters-declaration@0.0.9/node_modules/inspect-parameters-declaration/bin/cli.js" "$@"
fi
